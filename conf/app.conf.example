# 切记不要使用单引号, 只能使用双引号或者不使用引号, 注释需要单独一行
appname = binance_futures
# zh, en
language = zh
log = 1
debug = 0

[binance]
api_key = ""
api_secret = ""
# 本地代理(如果不需要改为"")
proxy_url = "http://127.0.0.1:7890"

[database]
# sqlite, mysql
driver = "sqlite"
path = "./db/coin.db?_journal_mode=WAL&_busy_timeout=5000"

# mysql
# driver = "mysql"
# username = ""
# password = ""
# host= ""
# port= ""
# dbname = ""

[ws]
# 使用 ws 管理仓位和订单，代替 http api，可有效避免 api 超限
futures_user_data = 1

[web]
# web端口
port = 3333
# 首页 path
index = zmkm
# jwt key
secret_key = 12321
# 用户名
username = admin
# 密码
password = admin
# 过期时间(hour)
expires = 24
# 重启服务按钮命令
commend_start = pm2 restart binance_futures
# 停止合约服务命令
commend_stop = pm2 stop binance_futures
# 查看 web 日志的命令 /pm2-log
commend_log = pm2 log binance_futures

[notification]
# dingding, slack
channel = dingding

[dingding]
# token
dingding_token = ""
# 触发关键词
dingding_word = "报警"

[slack]
slack_token = ""
slack_channel_id = ""

[external]
# 外部链接
links = [{"url": "url1", "title": "title1"}]